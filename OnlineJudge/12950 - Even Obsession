#include <bits/stdc++.h>
#define ii pair<int,int>

using namespace std;

vector<ii> adj[100010];
int dist[100010];
int dij(int s,int t){
    memset(dist,999999,sizeof dist);
    priority_queue<ii,vector<ii>,greater<ii>>pq;
    dist[s]=0;
    pq.push(ii(0,s));
    while(pq.size()){
        int u=pq.top().second;
        int w=pq.top().first;
        pq.pop();
        if(w>dist[u])continue;
        for(int i=0;i<adj[u].size();i++){
            int v=adj[u][i].first;
            int wv=adj[u][i].second;
            if(dist[u]+wv<dist[v]){
                dist[v]=dist[u]+wv;
                pq.push(ii(dist[v],v));
            }
        }
    }
    return dist[t];
}

int main()
{
    int n,m;
    while(cin>>n>>m){
        vector<ii> graf[n];
        for(int i=0;i<m;i++){
            int a,b,c;
            cin>>a>>b>>c;
            graf[a-1].push_back({b-1,c});
            graf[b-1].push_back({a-1,c});
        }
        for(int i=0;i<n;i++)
            for(auto [k,a]: graf[i])
                for(auto [w,b]: graf[k]){
                    adj[i].push_back({w,a+b});
                    adj[w].push_back({i,a+b});
                }


        int pos=-1;
        pos=dij(0,n-1);
        if(pos==1061109567)cout<<-1<<endl;
        else cout<<pos<<endl;
        for(int i=0;i<n;i++)adj[i].clear();
    }
}
